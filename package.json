{
   "name": "sbis3-builder",
   "version": "24.3100.0-BUILD",
   "private": true,
   "description": "Gulp-based builder for SBIS3 web projects",
   "files": [
      "gulpfile.js",
      "get-builder-hash.js",
      "gulp/",
      "lib/",
      "packer/",
      "resources/",
      "builderHashFile"
   ],
   "nyc": {
      "exclude": [
         "gulpfile.js",
         "copy-to-dest.js",
         "minify-dest.js",
         "test/**/*"
      ]
   },
   "scripts": {
      "prepare": "node get-builder-hash.js",
      "fastInstall": "node install/install.js",
      "verify": "npm run build:lint && npm run build:test",
      "prepareBuilder": "node install/minify-builder.js && node install/remove-platform-repos.js && node install/copy-to-dest.js --node_modules=true",
      "build:verify": "npm run build:minify && npm run build:lint && npm run build:test",
      "build:minify": "node install/minify-builder.js",
      "build:only": "node install/copy-to-dest.js && cd dest && npm i --production && cd ../ && node install/minify-builder.js --directory=dest",
      "build:test": "node ./node_modules/mocha/bin/mocha --exit --timeout 600000 -R xunit --reporter-options output=xunit-result.xml > xunit.log",
      "build:lint": "node ./node_modules/eslint/bin/eslint --no-color --no-eslintrc -c=.eslintrc -f=checkstyle . > eslint-report.xml",
      "test": "node ./node_modules/mocha/bin/mocha --timeout 600000",
      "test:coverage": "node ./node_modules/nyc/bin/nyc --reporter=html --all ./node_modules/.bin/mocha --timeout 600000",
      "lint": "node ./node_modules/eslint/bin/eslint --color --no-eslintrc -c=.eslintrc -f=stylish .",
      "lint:errors": "node ./node_modules/eslint/bin/eslint --color --no-eslintrc -c=.eslintrc -f=stylish --quiet .",
      "lint:fix": "node ./node_modules/eslint/bin/eslint --no-eslintrc -c=.eslintrc --fix .",
      "updateTypescriptDiagnosticMessages": "node ./install/update-tscdm.js"
   },
   "dependencies": {
      "async": "2.6.1",
      "autoprefixer": "9.5.0",
      "clean-css": "5.3.2",
      "escodegen": "2.0.0",
      "espree": "9.4.1",
      "estraverse": "5.3.0",
      "es5-ext": "0.10.53",
      "fs-extra": "8.1.0",
      "gulp": "4.0.2",
      "gulp-chmod": "3.0.0",
      "gulp-if": "3.0.0",
      "gulp-plumber": "1.2.1",
      "gulp-rename": "1.3.0",
      "humanize": "0.0.9",
      "less": "4.1.3",
      "map-stream": "0.0.7",
      "p-map": "1.2.0",
      "postcss": "7.0.1",
      "postcss-safe-parser": "4.0.1",
      "postcss-url": "7.3.2",
      "postcss-css-variables": "0.17.0",
      "strip-bom": "3.0.0",
      "tensor-xmldom": "0.2.0-b9",
      "through2": "2.0.3",
      "saby-typescript": "git+https://git.sbis.ru/saby/typescript.git#rc-24.3100",
      "saby-units": "git+https://git.sbis.ru/saby/Units.git#rc-24.3100",
      "sbis3-json-generator": "git+https://git.sbis.ru/root/sbis3-json-generator.git#rc-24.3100",
      "terser": "5.16.5",
      "vinyl": "2.2.0",
      "workerpool": "6.0.0",
      "semver": "6.3.0",
      "react": "17.0.1",
      "react-dom": "17.0.1",
      "postcss-discard-duplicates": "4.0.2",
      "svgo": "2.3.0",
      "rtlcss": "3.5.0",
      "source-map": "0.7.4",
      "tailwindcss": "3.4.1",
      "@tailwindcss/container-queries": "0.1.1",
      "html-minifier-terser": "7.2.0",
      "svgtofont": "3.17.6",
      "css-tree": "1.1.3",
      "jss": "10.9.0",
      "lightningcss": "1.22.0",
      "browserslist": "4.22.3",
      "hsl-to-hex": "1.0.0"
   },
   "devDependencies": {
      "@tensor-corp/eslint-config": "2.0.3",
      "@types/sinon": "7.5.0",
      "sinon": "7.5.0",
      "chai": "4.1.2",
      "chai-as-promised": "7.1.1",
      "eslint": "5.1.0",
      "eslint-plugin-eslint-comments": "3.0.1",
      "eslint-plugin-filenames": "1.3.2",
      "eslint-plugin-node": "7.0.1",
      "eslint-plugin-promise": "3.8.0",
      "mocha": "10.1.0",
      "nyc": "^15.1.0"
   },
   "engines": {
      "node": ">=10.14.2"
   }
}